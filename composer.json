{
    "name": "joomla/joomla-cms",
    "type": "project",
    "description": "Joomla CMS",
    "keywords": ["joomla", "cms"],
    "homepage": "https://github.com/joomla/joomla-cms",
    "license": "GPL-2.0+",
    "config": {
        "vendor-dir": "libraries/vendor"
    },
    "autoload": {
        "psr-4": {
            "Joomla\\"      :"libraries/joomla",
            "Joomla\\Cms\\" :"libraries/cms",
            ""              :["libraries/joomla","libraries/cms"]
        },
        "classmap": [
            "libraries/legacy/",
            "libraries/joomla/",
            "libraries/cms/",
            "libraries/idna_convert/",
            "administrator/components/com_config",
            "components/com_config"
        ],
        "files": [
            "libraries/platform.php",
            "libraries/joomla/factory.php",
            "libraries/joomla/language/text.php",
            "libraries/joomla/application/route.php",
            "libraries/phpass/PasswordHash.php",
            "bootstrap/legacy/helper.php",
            "bootstrap/legacy/toolbar.php",
            "bootstrap/legacy/subtoolbar.php"
        ]
    },
    "require-joomla_framework_packages_not_public_yet_remember_to_move_these_into_require_below": {
        "joomla/event": "~2.0"
    },
    "require": {
        "php": ">=5.3.10",
        "joomla/application": "~1.4",
        "joomla/di": "~1.2",
        "joomla/event": "~1.1",
        "joomla/registry": "~1.4 >=1.4.5",
        "joomla/session": "~1.2",
        "joomla/string": "~1.3",
        "joomla/uri": "~1.1",
        "joomla/utilities": "~1.3 >=1.3.3",
        "ircmaxell/password-compat": "1.*",
        "leafo/lessphp": "0.3.9",
        "phpmailer/phpmailer": "5.2.9",
        "symfony/yaml": "2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "phpunit/dbunit": "~1.3",
        "squizlabs/php_codesniffer": "~1.5",
        "pear/cache_lite": "1.7.16"
    }
}
